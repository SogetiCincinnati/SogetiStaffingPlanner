@model SogetiStaffingPlanner.Models.Client

@{
    ViewBag.Title = "Client Details";
}
<div class="container">
    <h2>Client Details</h2>

    <div>

        <hr />
        <dl class="dl-horizontal">
            <dt>
                @Html.DisplayNameFor(model => model.ClientName)
            </dt>

            <dd>
                @Html.DisplayFor(model => model.ClientSubbusiness)
            </dd>

            <dt>
                @Html.DisplayNameFor(model => model.LastModified)
            </dt>

            <dt>
                @Html.DisplayNameFor(model => model.LastModifiedUserId)
            </dt>



        </dl>
    </div>
    <div class="form-horizontal">
        <div class="col-md-offset-1 col-md-8">
            @Html.ActionLink("Edit", "Edit", new { id = Model.ClientId }, new { @class = "btn btn-primary", @role = "button" })
            <a class="btn btn-default" href="/clients" role="button">Cancel</a>
        </div>
    </div>

</div>
