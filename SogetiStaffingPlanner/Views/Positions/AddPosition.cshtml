<div id="app" class="main">
    <h1>Positions</h1>
    <!-- MISSING POSITION FILEDS NEED TO BE ADDED ASAP -->
    <table>
        <tr>
            <th>Name</th>
            <th>Number of Positions</th>
            <th>Duration</th>
            <th>Accepted Candidate</th>
        </tr>
        <!-- BEGIN VUE DYNAMIC TABLE -->
        <tr v-for="position in positions">
            <td>{{ position.PositionName }}</td>
            <td>{{ position.NumberOfPositions }}</td>
            <td>{{ position.Duration }}</td>
            <td>{{ position.AcceptedCandidate }}</td>
        </tr>
        <!-- END VUE DYNAMIC TABLE -->
    </table>
</div>
<script type="text/javascript" src="../../Scripts/App/Position/position.js"></script>

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
