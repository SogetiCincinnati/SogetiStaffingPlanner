<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    @*  <title>@ViewBag.Title - My ASP.NET Application</title>*@
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    @Scripts.Render("~/bundles/jquery")

    <link rel="stylesheet" href="../../Content/Site.css">
    <script type="text/javascript"
            src="../../Scripts/vue.min.js"></script>
    <title>Sogeti Staffing Planner</title>

</head>
<body>
    <div class='navBar'>
        <div id="logoContainer">
            <img class='navLogo' src="../../Images/logo.png" width='200px'>
        </div>
        <div id="navContainer">
            <!--<span class='navTitle'>Staffing Planner</span>-->
            <ul class='navItems'>
                <li><a href="/">Home</a></li>
                <li><a href="/clients/">Clients</a></li>
                <li><a href="/positions/">Positions</a></li>
                <li><a href="/opportunity/">Opportunities</a></li>
                <li><a href="/AccountExecutive/">AE</a></li>
                <li><a href="/Unit/">Unit</a></li>
                <li><a href="/Region/">Region</a></li>
            </ul>
        </div>
    </div>
    <div class="body-content">
        @RenderBody()
        <br /><br />
        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year - Sogeti</p>
        </footer>
    </div>

    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
